Card.Text>\n                    <Card.Text>{title}</Card.Text>\n                    <Card.Text style={{ color: \"grey\" }}>\n                      {area}\n                      {\"        \"}\n                      <a href=\"#\" style={{ fontWeight: \"bolder\" }}>\n                        129 links\n                      </a>\n                      {\"  |  \"} Strive School\n                    </Card.Text>\n                    <Button variant=\"primary\">Connect</Button>{\" \"}\n                  </Card.Body>\n                </Card>\n                <Card className=\"mt-5\">\n                  <Card.Body>\n                    <Card.Text\n                      style={{ fontSize: \"30px\", fontWeight: \"bolder\" }}\n                    >\n                      About\n                    </Card.Text>\n\n                    <Card.Text>{`${bio}`}</Card.Text>\n                  </Card.Body>\n                </Card>\n\n                {this.state.profile._id ? (\n                  //<ExperienceSection object={this.state.profile} />\n                  <ExperienceSection object={this.state.profile} id={this.props.match.params._id===\"me\"? this.state.profile._id : this.props.match.params._id} />\n                ) : null}\n              </Col>\n              <Col xs={4}>\n                <Aside />\n              </Col>\n            </Row>\n            <EditModalProfile\n              obj={this.state.profile}\n              show={this.state.editShow}\n              onHide={() => this.setState({ editShow: false })}\n              fetchProfile={this.fetchProfile}\n            />\n          </Container>\n        )}{\" \"}\n      </div>\n    );\n  }\n}\n","/Users/pierdomenicoapruzzese/strive-linkedin-bw2/src/Components/EditPostModal.jsx",["45"],"/Users/pierdomenicoapruzzese/strive-linkedin-bw2/src/Components/Aside.jsx",["46","47","48","49","50","51","52","53"],"/Users/pierdomenicoapruzzese/strive-linkedin-bw2/src/Api/auth.js",["54"],"/Users/pierdomenicoapruzzese/strive-linkedin